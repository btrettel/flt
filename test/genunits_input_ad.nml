&config
output_file     = "src/units_ad.f90"
base_units      = "m", "kg", "s"
type_definition = "type(ad)"
use_line        = "use prec, only: WP;use fmad, only: ad, sin, cos, tan, exp, log, tanh, atanh, abs, min, max"
kind_parameter  = "WP"
module_name     = "units_ad"
max_n_units     = 10

min_exponents = 0.0, 0.0, 0.0
max_exponents = 1.0, 0.0, 0.0
denominators  = 1, 1, 1

sqrt   = .false.
cbrt   = .false.
square = .false.
/

&seed_unit
label = "unitless"
e = 0.0, 0.0, 0.0
/

&seed_unit
label = "si_length"
e = 1.0, 0.0, 0.0
/
